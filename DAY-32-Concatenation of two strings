#include <iostream>
#include<conio.h>
#include<string.h>
int main()
{
	char str1[30]="Aditya ";
	char str2[30] = "Pateriya";  
	int i=0,stop;
	do	
	{ 
		stop=i++;
	}
	while(str1[i]!='\0');
    
	i=stop+1;
	
	for(int j = 0; str2[j] != '\0'; j++, i++)
		str1[i] = str2[j];

	str1[i] = '\0';
	std::cout<<"Resultant string is: "<< str1;
	getch();
}
